{
  "name": "@ifake/tree-path",
  "version": "0.0.1",
  "description": "Get tree path by the given condition.",
  "main": "dist/TreePath.min.js",
  "module": "dist/TreePath.min.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --config ./build/dev.js",
    "serve:example": "serve -l 8899 -n ./examples",
    "dev": "npm-run-all --parallel serve serve:example",
    "build": "cross-env NODE_ENV=production webpack --config ./build/prod.js",
    "prettier": "prettier --write --config .prettierrc.js \"src/**/*.{js,ts}\"",
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "jest",
    "test:analy": "jest --coverage"
  },
  "author": "biyuqi@163.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^2.3.8",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-chain": "^6.5.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts,tsx": [
      "prettier --write --config .prettierrc.js \"src/**/*.{js,ts}\"",
      "yarn run lint"
    ]
  }
}
